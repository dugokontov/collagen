<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <link rel="shortcut icon" href="<%= favicon %>" type="image/x-icon" />
    <link rel="stylesheet" href="/assets/collagen/css/bootstrap.min.css?<%= version %>" media="screen" />
    <% _.each(css, function(file) { %>
        <link rel="stylesheet" href="<%= file %>?<%= version %>" media="<%= /print/.test(file) ? 'print' : 'screen' %>" />
    <% }); %>
    <style>
      body {
        padding-top: 60px; /* 60px to make the container go all the way to the bottom of the topbar */
      }
    </style>
    <script src="/assets/collagen/js/jquery.min.js?<%= version %>"></script>
    <script src="/assets/collagen/js/bootstrap.min.js?<%= version %>"></script>
    <script src="/assets/bones/all.js?<%= version %>"></script>
    <% _.each(js, function(file) { %>
        <script src="<%= file %>?<%= version %>"></script>
    <% }); %>
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>
  <body>
    <%= navbar %>
    <div id="page" class="container clearfix">
      <div id="main"><%= main %></div>
    </div>
    <hr />
    <%= templates.Footer() %>
    <script><%= startup %></script>
  </body>
</html>
